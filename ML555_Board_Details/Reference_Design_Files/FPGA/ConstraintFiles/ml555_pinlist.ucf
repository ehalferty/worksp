NET  LVDSCLKMOD1_N    LOC  =  H20 ;  ## 
NET  SATA_MGT_GCLKP   LOC  =  J20 ;  ## ICS2 PLL LVDS or 125 MHz LVDS Oscillator
NET  SATA_MGT_GCLKN   LOC  =  J21 ;  ##  
NET  SFP_MGT_GCLKP    LOC  =  G15 ;  ## 125 MHz LVDS Oscillator
