NET  LVPECL_200M_N    LOC  =  L18 ;  ## 
NET  SMA_GCLKP        LOC  =  H17 ;  ## SMA Connector J10
NET  SMA_GCLKN        LOC  =  H18 ;  ## SMA Connector J11
